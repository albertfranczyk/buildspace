{
  "env": {
      "es6": true,
      "node": true
  },
  "plugins": ["prettier"],
  "extends": [
      "airbnb-base",
      "eslint:recommended",
      "plugin:prettier/recommended",
      "prettier"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "prettier/prettier": ["error"]
  }
}